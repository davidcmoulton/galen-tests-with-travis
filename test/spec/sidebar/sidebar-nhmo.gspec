@objects
  sidebarWrapper    css         .sidebar-wrapper
  sidebarNhmo       css         .article-belongs-to--nhmo
  sidebarNhmoTexts  css         .article-belongs-to--nhmo div

@ *
-------------------------------------------------------------
sidebarNhmoTexts
  inside: sidebarNhmo 135 px left

@ wide
-------------------------------------------------------------
sidebarNhmo
  width: 323px

@ medium
-------------------------------------------------------------
sidebarNhmo
  width: 260px

@ narrow
-------------------------------------------------------------
sidebarNhmo
  width: 100 % of sidebarWrapper/width
